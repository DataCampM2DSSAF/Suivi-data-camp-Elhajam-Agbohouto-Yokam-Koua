   ],
   "source": [
    "train = train.merge(building_metadata, on='building_id', how='left')\n",
    "alltrain = train.merge(weather_train, on=['site_id','timestamp'], how='left')\n",
    "del building_metadata,weather_train,train\n",
    "gc.collect()"
   ]
