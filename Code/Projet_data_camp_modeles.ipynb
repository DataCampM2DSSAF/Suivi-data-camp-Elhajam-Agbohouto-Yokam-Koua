   "metadata": {},
   "outputs": [],
   "source": [
    "def rec_heur(x):\n",
    "    \n",
    "    if x in np.arange(6, 19):\n",
    "        return 'journee'\n",
    "    \n",
    "    if x in np.arange(19, 23):\n",
    "        return 'nuit'\n",
    "    \n",
    "    if x in [23, 0, 1, 2, 3, 4, 5]:\n",
    "        return 'tard'"
   ]
  },
