   "source": [
    "def minMax(DF, listColumns, scaler):\n",
    "    df=DF.copy()\n",
    "    for col in listColumns:\n",
    "        df[col]=scaler.fit_transform(df[[col]])\n",
    "        \n",
    "    return df"
   ]
